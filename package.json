{
  "name": "home",
  "version": "1.0.0",
  "description": "ronan mccabe",
  "main": "server.js",
  "scripts": {
    "test": "lab -t 80 -r lcov > lcov.info",
    "start": "nf run nodemon server.js",
    "check": "npm run jshint && npm run coverage",
    "build": "browserify -t uglifyify ./js/public/main.js | uglifyjs -o ./build/mainBundle.js && browserify -t uglifyify ./js/public/analytics.js | uglifyjs -o ./build/analyticsBundle.js",
    "watch:mainjs": "watchify ./js/public/main.js -o ./build/mainBundle.js",
    "watch:analyticsjs": "watchify ./js/public/analytics.js -o ./build/analyticsBundle.js",
    "watch": "npm run watch:analyticsjs & npm run watch:mainjs",
    "coverage": "lab -t 80 -r console",
    "coverage-html": "lab -t 100 -r html > tests.html",
    "jshint": "jshint -c .jshintrc --exclude-path .jshintignore .",
    "postinstall": "npm run build",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=03ec9a93ba205a0eb5c27aedea5ca5f262411b24336566fba622ae012b4d3886 codeclimate-test-reporter < lcov.info"
  },
  "pre-commit": [
    "jshint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wallcrawler/home.git"
  },
  "keywords": [
    "ronan",
    "mccabe",
    "london",
    "ireland"
  ],
  "author": "ronan mccabe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wallcrawler/home/issues"
  },
  "homepage": "https://github.com/wallcrawler/home#readme",
  "dependencies": {
    "angular": "^1.4.4",
    "browserify": "^10.2.4",
    "foreman": "^1.4.1",
    "good": "^6.2.0",
    "good-http": "^5.0.0",
    "handlebars": "^3.0.3",
    "hapi": "^8.6.1",
    "jquery": "^2.1.4",
    "leaflet": "^0.7.3",
    "redis": "^0.12.1",
    "uglifyify": "^3.0.1",
    "uglifyjs": "^2.4.10"
  },
  "devDependencies": {
    "code": "^1.4.1",
    "codeclimate-test-reporter": "^0.1.0",
    "jshint": "^2.8.0",
    "lab": "^5.11.1",
    "pre-commit": "^1.0.10",
    "shot": "^1.5.3",
    "watchify": "^3.3.1"
  }
}
